{
  "name": "Business Journal Rankings",
  "description": "This extension determines whether a highlighted journal name is an FT-50 journal and its ABDC and ABS classification.",
  "version": "0.12",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/AC_LOGO16.png",
      "32": "/images/AC_LOGO32.png",
      "48": "/images/AC_LOGO48.png",
      "128": "/images/AC_LOGO128.png"
    }
  },
  "icons": {
    "16": "/images/AC_LOGO16.png",
    "32": "/images/AC_LOGO32.png",
    "48": "/images/AC_LOGO48.png",
    "128": "/images/AC_LOGO128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+J"
      }
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}